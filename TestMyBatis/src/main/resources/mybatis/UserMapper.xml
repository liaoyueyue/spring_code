<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.testmybatis.mapper.UserMapper">
    <select id="getUserById" resultType="com.example.testmybatis.entity.UserInfo">
        select *
        from userinfo
        where id = ${userId}
    </select>

    <select id="getUserInfoAll" resultType="com.example.testmybatis.entity.UserInfo">
        select *
        from userinfo
    </select>
</mapper>